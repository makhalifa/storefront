  "scripts": {
    "start": "nodemon ./src/index.ts",
    "build": "npx tsc",
    "start:prod": "npm run build && node dist/index.js",
    "lint": "eslint .",
    "lint:fix": "eslint --fix",
    "format": "prettier --write 'src/**/*.ts'",
    "test" : "set ENV=test&& npm run testdb-up && npx tsc && jasmine && npm run testdb-dwon",
